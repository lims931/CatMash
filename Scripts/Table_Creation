USE [CatMashDB]
GO

CREATE TABLE T_Cat (
    CatId int IDENTITY(1,1) PRIMARY KEY,
    CatUrl VARCHAR (max) NOT NULL
);




CREATE TABLE T_Matche (
    MatcheId int IDENTITY(1,1) PRIMARY KEY,
	LeftCatId int NOT NULL,
	RightCatId int NOT NULL,
	MatchResult VARCHAR (10) NOT NULL
	CONSTRAINT [FK_Vote_WonMatches_CatId] FOREIGN KEY ([WinnerId]) REFERENCES [T_Cat] ([CatId]),
	CONSTRAINT [FK_Vote_LostMatches_CatId] FOREIGN KEY ([LooserId]) REFERENCES [T_Cat] ([CatId])
);
